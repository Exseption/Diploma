table.table.table-striped
    thead
        tr
            th #
            th First Name
            th Last Name
            th Username
            th Usertype
            th Email
    tbody
        tr
            th(scope='row') 1
            td #{user.firstName}
            td #{user.lastName}
            td #{user.username}
            td #{user.userType}
            td #{user.email}